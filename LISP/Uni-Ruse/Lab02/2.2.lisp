;;;; 2 задача: Да се дефинира функция, която 
;;;;;	- по зададени две числа, 
;;;;;	- връща утроено по–малкото от двете, 
;;;;;	- а ако са равни – връща кое да е от тях.

;; Define function
(defun myFunction (a b)
  (cond
    ((> a b) (* 3 b))
    ((> b a) (* 3 a))
    (t a )))

;; Call function
(myFunction 4 3)
