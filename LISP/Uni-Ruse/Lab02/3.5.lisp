;;;; 5 задача: Да се дефинира функция, която по зададена
;;;; стойност на w, определя g(w) по функцията:
;;;; g(w) = 
;;;; {
;;;;  -w, w <= -20
;;;;  2w, -20 < w < 3
;;;;  (w-1)^2, w >= 3
;;;; }

(defun g (w)
  (cond
    ((<= w -20) (* -1 w))
    ((< -20 w 3) (* 2 w))
    ((>= w 3) (expt (- w 1) 2))))
