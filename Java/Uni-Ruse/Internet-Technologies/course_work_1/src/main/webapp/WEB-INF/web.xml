  <servlet>
  	<servlet-name>FormDataFileTableGenerator</servlet-name>
  	<display-name>FormDataFileTableGenerator</display-name>
  	<description></description>
  	<servlet-class>servlets.FormDataFileTableGenerator</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>FormDataFileSaver</servlet-name>
  	<url-pattern>/FormDataFileSaver</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FormDataFileTableGenerator</servlet-name>
  	<url-pattern>/FormDataFileTableGenerator</url-pattern>
  </servlet-mapping>
</web-app>